{
  "manifest_version": 2,
 
  "name": "Paperback: WorkFlowy Modernised",
  "version": "0.1.3",
  "description": "A focused new interface for your lists, notes and ideas.",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-3.2.1.min.js", "content.js"],
    "matches": ["https://workflowy.com/*"]
  }],

  "browser_action": {
  		"default_icon": "icon.png"
	},

  "web_accessible_resources": [
      "assets/*"
    ]
  
}