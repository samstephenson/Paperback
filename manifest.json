{
  "manifest_version": 2,
 
  "name": "Paperback: Workflowy modernised",
  "version": "0.2.2",
  "description": "A cleaner, more colorful interface for WorkFlowy",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-3.2.1.min.js", "content.js"],
    "matches": ["https://workflowy.com/*"]
  }],

  "browser_action": {
  		"default_icon": "icon.png"
	},

  "web_accessible_resources": [
      "assets/*"
    ]
  
}